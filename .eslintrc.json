{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/jsx-no-target-blank": "off",
    "no-unused-vars": [
      "error",
      { "vars": "all", "args": "after-used", "ignoreRestSiblings": true }
    ],
    "react/prop-types": "off",
    "react/no-unescaped-entities": "off",
    "react-refresh/only-export-components": "off"
  }
}
